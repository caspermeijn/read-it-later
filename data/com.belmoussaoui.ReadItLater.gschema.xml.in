<?xml version="1.0" encoding="utf-8"?>
<schemalist>
  <schema path="/com/belmoussaoui/ReadItLater/" id="@app-id@" gettext-domain="@gettext-package@">
    <key name="window-width" type="i">
      <default>-1</default>
      <summary>Default window width</summary>
    </key>
    <key name="window-height" type="i">
      <default>-1</default>
      <summary>Default window height</summary>
    </key>
    <key name="window-x" type="i">
      <default>-1</default>
      <summary>Default window x position</summary>
    </key>
    <key name="window-y" type="i">
      <default>-1</default>
      <summary>Default window y position</summary>
    </key>
    <key name="is-maximized" type="b">
      <default>false</default>
      <summary>Default window maximized behaviour</summary>
    </key>
    <key name="username" type="s">
      <default>''</default>
      <summary>Logged in username</summary>
    </key>
    <key name="latest-sync" type="i">
      <default>0</default>
      <summary>Latest successful sync in ms</summary>
    </key>
    <key name="dark-mode" type="b">
      <default>false</default>
      <summary>Whether we should use a dark theme or not</summary>
    </key>
  </schema>
</schemalist>
